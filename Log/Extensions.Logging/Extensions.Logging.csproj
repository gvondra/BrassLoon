<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;netstandard2.0</TargetFrameworks>
		<Nullable>disable</Nullable>
		<ImplicitUsings>disable</ImplicitUsings>
		<AssemblyName>BrassLoon.Extensions.Logging</AssemblyName>
		<RootNamespace>BrassLoon.Extensions.Logging</RootNamespace>
		<Authors>Greg Vondra</Authors>
		<Company />
		<Product>Brass Loon</Product>
		<PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
		<LangVersion>10</LangVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>1.0.0-beta.8</Version>
		<AssemblyVersion>1.0.0.8</AssemblyVersion>
		<PackageReleaseNotes></PackageReleaseNotes>
		<PackageProjectUrl>https://github.com/gvondra/BrassLoon</PackageProjectUrl>
		<RepositoryUrl>https://github.com/gvondra/BrassLoon.git</RepositoryUrl>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DebugType>full</DebugType>
		<DebugSymbols>true</DebugSymbols>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DebugType>none</DebugType>
		<DebugSymbols>false</DebugSymbols>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="BrassLoon.Interface.Account" Version="[2.2.0,2.3)" />
	  <PackageReference Include="BrassLoon.Interface.Log" Version="2.2.0-prerelease.*" />
	  <PackageReference Include="BrassLoon.RestClient" Version="[3.1.0,3.2)" />
	  <PackageReference Include="Microsoft.Extensions.Logging" Version="[6.0.0,6.1)" />
	  <PackageReference Include="Polly" Version="[7.2.3,7.3)" />
	  <PackageReference Include="Polly.Caching.Memory" Version="[3.0.2,3.1)" />
	</ItemGroup>

</Project>
